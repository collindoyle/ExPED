#ifndef __FONTCACHE
#define __FONTCACHE

#include "pedlist.h"
#include "pedfontdes.h"


typedef struct _fontcache {
	pedlist * font_list;
} fontcache;

fontcache * initialize_font_cache (fontcache * cache);




#endif
